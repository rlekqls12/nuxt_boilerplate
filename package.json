{
  "name": "nuxt_boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "pc:loc": "cross-env NODE_ENV=loc DEVICE=pc nuxt",
    "mo:loc": "cross-env NODE_ENV=loc DEVICE=mo nuxt",
    "pc:loc:build": "cross-env NODE_ENV=loc DEVICE=pc nuxt build",
    "mo:loc:build": "cross-env NODE_ENV=loc DEVICE=mo nuxt build",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix"
  },
  "dependencies": {
    "@nuxt/babel-preset-app": "^2.16.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dayjs": "^1.4.1",
    "body-parser": "^1.20.2",
    "core-js": "^3.25.3",
    "nuxt": "^2.15.8",
    "nuxt-healthcheck": "^1.0.1",
    "vue": "^2.7.10",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/runtime-corejs3": "^7.21.0",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "prettier": "^2.7.1"
  }
}
